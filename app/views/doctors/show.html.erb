<h1><%= @doctor.name %></h1>
<h2>Department: <%= @doctor.department %></h2>

  <%= @appointments.each do |appt| %>
    <li>Patient name: <%= link_to appt.patient_name, patient_path(@patient) %></li>
    <li>Appointment Time: <%= appt.appointment_datetime.strftime('%B %d, %Y at %H:%M') %></li>
    <% end %>
